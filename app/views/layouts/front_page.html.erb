	<%= javascript_include_tag 'front_page'%>
	
	
</head>
<body>
	
	<div style='display:none' id='featured_data'>
	  <% @featured_projects.each do |project|%>
	    <% picture = project.current_displayed_featured_pictures.first %>
	    <div class='featured'>
	      <span class='featured_image'><%= picture.feature_image_url %></span>
	      <span class='featured_title'><%= link_to project.title, project_url(project)  %></span>
	      <span class='featured_thumbnail'><%= picture.feature_image_thumbnail_url %></span>
	    </div>
	  <% end %>
	</div>
	<div class="ef-slider-overlay"></div>
	
	<div id="tray-button"><div><span class="ef-tooltip">Toggle Fullscreen</span></div></div>
	
  <!-- <div id="ef-topbar-button"><span class="ef-tooltip">More Info</span></div> -->
	
	<div id="ef-header" class="ef-toggled">
		<div class="ef-logo-wrapper ef-toggled">
			<a href="#home" class="ef-logo"><img src="<%= FRONTPAGE_IMAGE_URL[:logo]%>" alt=""></a>
		</div>
		<div id="ef-mobile-menu-wrapper"></div>		
	</div>

<!-- Navigation -->
	
	<div class="ef-nav-wrapper" style='z-index:999;'>	
    <span class="ef-menu-tab"></span>    
		
		<div class="ef-menu-wrapper">			
			<ul class="ef-navmenu">
				<li class="ef-menu-cur"><a href="#home">Home</a></li>
				<li><a href="#about">About</a></li>				
        <!-- <li><a href="#portfolio">Portfolio</a></li> -->
				<li><a href="#blog">Portfolio</a></li>
				<li><a href="#contact">Contact</a></li>
			</ul>
		</div>

	</div>

<!-- Home page -->
	
	<div class="ef-slider ef-content current-section" id="home" >
			
		<div id="ef-slider">					

			<div id="slidecaption"></div>			
			
			<div id="slider-controls-wrapper">
				<div class="ef-controls-inner">
				
					<div id="thumb-tray" class="load-item">
						<div class="thumb-nav">
							<div id="thumb-back"></div>
							<div id="thumb-forward"></div>
						</div>
					</div>				
					
				</div>
			</div>
			
			<div class="dots-play"> 
				<ul id="slide-list"></ul> 
			</div>
			
		</div>

		<div class="ef-inner-header ef-toggled">
			
			<div class="ef-page-details">
				<h1>Hello, Dear Friends!!!</h1>
				<div class="ef-description">
					<p>I am <span>Sniper</span> - Premium Site Template for Photographers</p>					
				</div>
				<a class="ef-next-page" href="#about"></a>
			</div>
			
			<!--Control Bar-->
			<div id="controls-wrapper" class="load-item">
				<div id="controls">				
					
					<!--Arrow Navigation-->
					<a id="prevslide" class="load-item"><span class="ef-tooltip">Previous Slide</span></a>
					
					<a id="play-button"><img id="pauseplay" src="<%= FRONTPAGE_IMAGE_URL[:pause]%>" alt=""/><span class="ef-tooltip">Play/Pause</span></a>
					
					<a id="nextslide" class="load-item"><span class="ef-tooltip">Next Slide</span></a>				
					
				</div>
			</div>
			
			<div id="progress-back" class="load-item">			
			<!--Time Bar-->			
				<div id="progress-bar"></div>
			</div>
			<span class="gradi"></span>
			
		</div>
		<div class="ef-fold"></div>
	</div>
	
<!-- About page -->
	
  <div class="ef-content" id="about">
    <div class="ef-inner-header ef-toggled">
      
      <div class="ef-page-details">       
        <h1>About</h1>
        <div class="ef-description">
          <p>You can write <span>short description</span> of a page here. Freaky awesome!</p>
        </div>
        
        <a class="ef-next-page" href="#blog"></a>
        <a class="ef-prev-page" href="#home"></a>
        
      </div>
      <span class="gradi"></span>
    </div>
    
    <div class="ef-column ef-fold">       
      <div class="ef-col-inner">
        <div class="ef-page-content clearfix">
          <div class="shortcode-wrapper">
          
            <div class="one_fourth">
              <h4>Some words about me</h4>
              
              <p class="ef-small-title">Nemo enim ipsam voluptatem</p>
              
              <div class="proj-img">
                <img src="<%= FRONTPAGE_IMAGE_URL[:basic_pic] %>" alt="">
                <div class="ef-overlay">
                  <a class="lb" href="http://vimeo.com/47767166" title="Your description here"></a>
                </div>
              </div>
              
              <hr class="clear">
              
              <p><strong>This is simple example of image with video in lightbox</strong></p>
              
              <p><span class="ef-highlight hl-theme">Sed ut perspiciatis</span> unde omnis iste natus error sit voluptatem accusantium doloremque laudantium, totam rem aperiam, eaque ipsa quae ab illo inventore veritatis et quasi architecto beatae vitae dicta sunt explicabo.
              </p>
              
            </div>
            
            <div class="one_half">
              <h4>Instagram</h4>
              
              <div class='instagram'>
              </div>
  
            </div>
            
            <div class="shortcode-clear"></div>
             
            <div class="one_fourth widget_twitter">
              <h4>Tweet</h4>
              <div class="ef-tweet">
              </div> 
            </div>
            
          </div>
        </div>
      </div>
    </div>        
      
  </div>
 

<!-- Blog -->
	
  <div class="ef-content" id="blog">
    <div class="ef-inner-header ef-toggled">
      
      <div class="ef-page-details">       
        <h1>Blog</h1>
        <div class="ef-description">
          <p>This is a <span>grid blog</span>. You can use image, slider or video as thumbnail.</p>
        </div>
        
        <a class="ef-next-page" href="#contact"></a>
        <a class="ef-prev-page" href="#about"></a>
        
      </div>
      <span class="gradi"></span>
    </div>
    
    <div class="ef-column ef-fold"> 
      <div class="ef-col-inner">
        <div class="ef-page-content bottom-2_4em clearfix">
                  
          <div class="clear"></div>
          
          <div id="ef-blog">
            <div class="shortcode-wrapper">                 
              <% @projects.each do |project| %>
                <%= render :partial => 'home/front_page/entry', :locals => {
                  :project => project
                }%>
              <% end %> 
            </div>    
          </div>
        </div>
      </div>
      
      <!-- <a class="ef-archive" href="#">More Posts</a> -->
      
    </div>

  </div>

<!-- Contact -->
	
	<div id="contact" class="ef-content">
	
		<div class="ef-inner-header ef-toggled">
			
			<div class="ef-page-details">				
				<h1>Contact</h1>
				<div class="ef-description">
					<p>Here you can add your <span>contact info</span> and set unlimited number of locations</p>
				</div>
				
				<a class="ef-prev-page" href="#blog"></a>
				
			</div>
			<span class="gradi"></span>
		</div>
		
		<div class="ef-map"></div>
		
		<div class="ef-column ef-fold">			
							
			<div class="ef-col-inner">
				
				<div class="ef-sidebar-wrapper clearfix">
				
					<div class="ef-sidebar">
						<div class="shortcode-wrapper">
							
							<div class="one_half bottom-2_4em">
							
								<h5>Get in touch with Us</h5>
								
								<ul class="clearlist bottom-1_5em">
									<li><strong>Address:</strong> Jl. Tanjung Duren Utara IV/225, Jakarta Barat - Indonesia</li>
									<li><strong>Phone:</strong> +62-8588 57 9797 0</li>
									<li><strong>Email:</strong> me@willliam-photography.com</li>
									<li><strong>Website:</strong> www.willliam-photography.com</li>
								</ul>				
								
								<h5>Our offices:</h5>
								
								<span id="ef-office-1" class="ef-cur-office">Primary office</span>
                <!-- <span id="ef-office-2">Secondary office</span> -->
							
							</div>
							
							<div class="one_half bottom-2_4em"> 
                <div class="ef-list"></div> 
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>				
			
	</div>
	
